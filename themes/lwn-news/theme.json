{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "customTemplates": [],
  "patterns": [],
  "settings": {
    "appearanceTools": true,
    "layout": {
      "contentSize": "1000px",
      "wideSize": "1400px"
    },
    "useRootPaddingAwareAlignments": true,
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "palette": [
        {
          "slug": "base",
          "name": "Base",
          "color": "#ffffff"
        },
        {
          "slug": "contrast",
          "name": "Contrast",
          "color": "#2a2a2a"
        },
        {
          "slug": "accent-1",
          "name": "Accent 1",
          "color": "#ac3e43"
        },
        {
          "slug": "accent-2",
          "name": "Accent 2",
          "color": "#348d34"
        }
      ]
    },
    "typography": {
      "dropCap": false,
      "textTransform": false,
      "defaultFontSizes": false,
      "customFontSize": false,
      "lineHeight": true,
      "fontWeight": true,
      "letterSpacing": false,
      "textDecoration": false,
      "writingMode": false,

      "fontSizes": [
        {
          "slug": "extra-small",
          "size": "0.8rem",
          "name": "XS"
        },
        {
          "slug": "small",
          "size": "0.9rem",
          "name": "S"
        },
        {
          "slug": "regular",
          "size": "1rem",
          "name": "Regular"
        },
        {
          "slug": "large",
          "size": "1.5rem",
          "name": "L"
        },
        {
          "slug": "extra-large",
          "size": "2.5rem",
          "name": "XL"
        },
        {
          "slug": "extra-extra-large",
          "size": "3.5rem",
          "name": "XXL"
        }
      ],
      "fontFamilies": [
        {
          "fontFamily": "Zain",
          "name": "Zain",
          "slug": "zain",
          "fontFace": [
            {
              "fontFamily": "Zain",
              "fontWeight": "200",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-200.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "300",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-300.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-400.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "700",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-700.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "800",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-800.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "900",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/zain/zain-900.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "300",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/zain/zain-italic-300.woff2"]
            },
            {
              "fontFamily": "Zain",
              "fontWeight": "400",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/zain/zain-italic-400.woff2"]
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "spacing": {
      "padding": {
        "bottom": "0",
        "left": "10px",
        "right": "10px",
        "top": "0"
      }
    }
  }
}
